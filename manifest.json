{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "DoneLikes - Automatically Like for Instagram",
	"description": "Automatically like Instagram posts in Instagram Pods/Engagement Groups",
	"version": "2.0.1",
	"icons": {
		"128": "icon_128.png",
		"48": "icon_48.png",
		"16": "icon_16.png"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "interface.html",
		"default_title": "DoneLikes"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"notifications",
		"storage",
		"unlimitedStorage",
		"https://*.instagram.com/*",
		"http://*.instagram.com/*",
		"https://boostextensions.com/autofollow/emailverify.php",
		"https://boostextensions.com/autofollow/sendmanagesubemail.php"
	]
}